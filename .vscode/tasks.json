{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: x86_64-elf-gcc build active file",
            "command": "make",
            "args": [
                "${fileDirname}/${fileBasenameNoExtension}.o"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "label": "debug-qemu",
            "command": "make clean debug-qemu-start",
            "type": "shell",
            "isBackground": true,
            "problemMatcher": {
                "pattern": {
                    "regexp": "^qemu-system-x86_64",
                    "line": 1,
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "^qemu-system-x86_64",
                    "endsPattern": "^qemu-system-x86_64"
                }
            }
        }
    ],
    "version": "2.0.0"
}